{
  "containerDefinitions": [
    {
      "name": "meilisearch",
      "image": "471438904144.dkr.ecr.us-east-1.amazonaws.com/aturian_meilisearch:latest",
      "portMappings": [
        {
          "containerPort": 3100,
          "hostPort": 3100
        },
        {
          "containerPort": 7700,
          "hostPort": 7700
        }
      ],
      "environment": [
        {
          "name": "TITLE",
          "value": "Meilisearch"
        },
        {
          "name": "AWS_REGION",
          "value": "us-east-1"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "awslogs-meilisearch-app",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "awslogs-meilisearch"
        }
      }
    }
  ],
  "executionRoleArn": "ecsTaskExecutionRole",
  "family": "meilisearch",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "1024"
}
