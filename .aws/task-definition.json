{
  "containerDefinitions": [
    {
      "name": "meilisearch",
      "image": "471438904144.dkr.ecr.us-east-1.amazonaws.com/aturian_meilisearch:553b08d4cde1176a4ea01ddf9bce0e02d49365a2",
      "portMappings": [
        {
          "containerPort": 1337,
          "hostPort": 1337
        },
        {
          "containerPort": 7700,
          "hostPort": 7700
        }
      ],
      "secrets": [
        {
          "name": "AWS_ACCESS_KEY_ID",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:471438904144:secret:gitaction-A5XYKK"
        },
        {
          "name": "AWS_SECRET_ACCESS_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:471438904144:secret:gitaction-A5XYKK"
        }
      ],
      "environment": [
        {
          "name": "TITLE",
          "value": "Meilisearch"
        },
        {
          "name": "AWS_REGION",
          "value": "us-east-1"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "awslogs-meilisearch-app",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "awslogs-meilisearch"
        }
      }
    }
  ],
  "executionRoleArn": "ecsTaskExecutionRole",
  "family": "meilisearch",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "1024"
}
